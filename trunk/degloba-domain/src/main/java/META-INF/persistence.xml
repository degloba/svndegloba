<?xml version="1.0" encoding="UTF-8" ?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"    
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"    
xsi:schemaLocation="http://java.sun.com/xml/ns/persistence        
http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd" version="2.0">   
 
     
	<persistence-unit name="transactions-optional">        
	<provider>org.datanucleus.api.jpa.PersistenceProviderImpl</provider>
		<class>com.degloba.domain.canonicalmodel.publishedlanguage.AggregateId</class>
		<class>com.degloba.domain.support.BaseAggregateRoot</class>
		<class>com.degloba.domain.canonicalmodel.publishedlanguage.ClientData</class>
		<class>com.degloba.domain.sharedkernel.Probability</class>
		<class>com.degloba.domain.BaseEntity</class>	
		<class>com.degloba.domain.support.BaseEntity</class>
		<class>com.degloba.domain.support.AbstractEntity</class>
		<class>com.degloba.domain.AbstractEntity</class>
		<class>com.degloba.domain.LegacyEntity</class>
		<class>com.degloba.domain.event.StoredEvent</class>
		<class>com.degloba.domain.Value</class>
		<class>com.degloba.domain.entity.MyEntity</class>
        <exclude-unlisted-classes/>  
		<properties>            
			<property name="datanucleus.NontransactionalRead" value="true"/>            
			<property name="datanucleus.NontransactionalWrite" value="true"/>            
			<property name="datanucleus.ConnectionURL" value="googlestorage"/>      	      
		</properties>    
	</persistence-unit>	
	
</persistence>
