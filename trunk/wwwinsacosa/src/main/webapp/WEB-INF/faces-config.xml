<?xml version="1.0"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
 <converter>
  <converter-id>DateConverter</converter-id>
  <converter-class>com.degloba.converters.DateConverter</converter-class>
 </converter>
 <managed-bean>
  <managed-bean-name>ciut</managed-bean-name>
  <managed-bean-class>com.insacosa.vo.CiutatsForm</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>tipusForm</managed-bean-name>
  <managed-bean-class>com.insacosa.vo.TipusForm</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>inmobleForm</managed-bean-name>
  <managed-bean-class>com.insacosa.vo.InmobleForm</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>userForm</managed-bean-name>
  <managed-bean-class>com.insacosa.vo.UserForm</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>compradorForm</managed-bean-name>
  <managed-bean-class>com.insacosa.vo.CompradorForm</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>registreComprador</managed-bean-name>
  <managed-bean-class>com.insacosa.vo.RegistreComprador</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>solicitudForm</managed-bean-name>
  <managed-bean-class>com.insacosa.vo.SolicitudForm</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>targetaCreditForm</managed-bean-name>
  <managed-bean-class>com.insacosa.vo.TargetaCreditForm</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>objecte</managed-bean-name>
  <managed-bean-class>com.insacosa.interfaces.Objecte</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <!-- Login - Register -->
 <managed-bean>
  <managed-bean-name>LoginForm</managed-bean-name>
  <managed-bean-class>com.insacosa.vo.LoginForm</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>RetrievePasswordForm</managed-bean-name>
  <managed-bean-class>com.insacosa.vo.RetrievePasswordForm</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>fotoForm</managed-bean-name>
  <managed-bean-class>com.insacosa.vo.FotoForm</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <from-view-id>/xhtml/header.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>registreVenedor</from-outcome>
   <to-view-id>/xhtml/registreVenedor.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>loginVenedor</from-outcome>
   <to-view-id>/xhtml/login_body.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/xhtml/menu.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>buscarInmoble</from-outcome>
   <to-view-id>/xhtml/buscarInmoble.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>conectarPaypal</from-outcome>
   <to-view-id>/xhtml/tarjeta_credit.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>filtre</from-outcome>
   <to-view-id>/xhtml/filtrarPerTipus.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>filtre2</from-outcome>
   <to-view-id>/xhtml/ss.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/xhtml/index.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>registreVenedor</from-outcome>
   <to-view-id>/xhtml/registreVenedor.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>loginVenedor</from-outcome>
   <to-view-id>/xhtml/loginVenedor.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-action>#{LoginForm.validUser}</from-action>
   <from-outcome>success</from-outcome>
   <to-view-id>/xhtml/index.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-action>#{LoginForm.validUser}</from-action>
   <from-outcome>failure</from-outcome>
   <to-view-id>/xhtml/index.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>useraccount</from-outcome>
   <to-view-id>/user/useraccount.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>forgotpassword</from-outcome>
   <to-view-id>/user/forgotpassword.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-action>#{userForm.editProfile}</from-action>
   <from-outcome>editSuccess</from-outcome>
   <to-view-id>/user/useraccount.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-action>#{LoginForm.logout}</from-action>
   <from-outcome>logoutsuccess</from-outcome>
   <to-view-id>/xhtml/index.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>solicitudsUsuaris</from-outcome>
   <to-view-id>/xhtml/solicitudsUsuaris.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>inmoblesUsuari</from-outcome>
   <to-view-id>/xhtml/inmoblesVenedor.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <!-- Login - Register -->
 <navigation-rule>
  <from-view-id>/user/login.xhtml</from-view-id>
  <navigation-case>
   <from-action>#{LoginForm.validUser}</from-action>
   <from-outcome>success</from-outcome>
   <to-view-id>/xhtml/index.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-action>#{LoginForm.validUser}</from-action>
   <from-outcome>failure</from-outcome>
   <to-view-id>/xhtml/index.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>forgotpassword</from-outcome>
   <to-view-id>/user/forgotpassword.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>useraccount</from-outcome>
   <to-view-id>/user/useraccount.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/user/forgotpassword.xhtml</from-view-id>
  <navigation-case>
   <from-action>#{RetrievePasswordForm.check}</from-action>
   <from-outcome>passwordsendingsuccess</from-outcome>
   <to-view-id>/user/successpwdsend.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-action>#{RetrievePasswordForm.check}</from-action>
   <from-outcome>passwordsendingfail</from-outcome>
   <to-view-id>/user/forgotpassword.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/user/success.xhtml</from-view-id>
  <navigation-case>
   <from-action>#{LoginForm.logout}</from-action>
   <from-outcome>logoutsuccess</from-outcome>
   <to-view-id>/xhtml/index.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-action>#{userForm.editProfile}</from-action>
   <from-outcome>editSuccess</from-outcome>
   <to-view-id>/user/useraccount.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>changePassword</from-outcome>
   <to-view-id>/user/changepassword.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/user/changepassword.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>success</from-outcome>
   <to-view-id>/user/changepasswordsuccess.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>failure</from-outcome>
   <to-view-id>/user/changepassword.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>gotomain</from-outcome>
   <to-view-id>/user/success.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/user/useraccount.xhtml</from-view-id>
  <navigation-case>
   <from-action>#{userForm.saveUser}</from-action>
   <from-outcome>successUserRegistration</from-outcome>
   <to-view-id>/xhtml/index.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-action>#{userForm.saveUser}</from-action>
   <from-outcome>successUserUpdate</from-outcome>
   <to-view-id>/xhtml/index.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-action>#{userForm.saveUser}</from-action>
   <from-outcome>error</from-outcome>
   <to-view-id>/user/useraccount.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/xhtml/inmoblesVenedor.xhtml</from-view-id>
  <navigation-case>
   <from-action>#{LoginForm.logout}</from-action>
   <to-view-id>/xhtml/index.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-action>#{userForm.editProfile}</from-action>
   <from-outcome>editSuccess</from-outcome>
   <to-view-id>/user/useraccount.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>changePassword</from-outcome>
   <to-view-id>/user/changepassword.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>filtre</from-outcome>
   <to-view-id>/xhtml/filtrarPerTipus.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>filtre2</from-outcome>
   <to-view-id>/xhtml/ss.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>inmoblesUsuari</from-outcome>
   <to-view-id>/xhtml/inmoblesVenedor.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/xhtml/buscarInmoble.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>filtre</from-outcome>
   <to-view-id>/xhtml/filtrarPerTipus.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>filtre2</from-outcome>
   <to-view-id>/xhtml/ss.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>gVista</from-outcome>
   <to-view-id>/xhtml/gVista.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/xhtml/filtrarPerTipus.xhtml'</from-view-id>
  <navigation-case>
   <from-outcome>inmoblesUsuari</from-outcome>
   <to-view-id>/xhtml/inmoblesVenedor.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <validator>
  <validator-id>DateValidator</validator-id>
  <validator-class>com.insacosa.validadors.DateValidator</validator-class>
 </validator>
 <validator>
  <validator-id>EmailValidator</validator-id>
  <validator-class>com.insacosa.validadors.EmailValidator</validator-class>
 </validator>
 <validator>
  <validator-id>PhoneNumberValidator</validator-id>
  <validator-class>com.insacosa.validadors.PhoneNumberValidator</validator-class>
 </validator>
</faces-config>
