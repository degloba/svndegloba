<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2"
              xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
 <name>degloba</name>
 <converter>
  <converter-id>listShuttleconverter</converter-id>
  <converter-class>listShuttleConverter</converter-class>
 </converter>

 <managed-bean>
  <managed-bean-name>frameworks</managed-bean-name>
  <managed-bean-class>frameworks</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
  <managed-property>
   <property-name>frameworks</property-name>
   <list-entries/>
  </managed-property>
 </managed-bean>
 <!--  MODALPANELS  -->
 <!--  <managed-bean>
  <managed-bean-name>modalPanels</managed-bean-name>
  <managed-bean-class>com.degloba.webui.JSF.modalPanels</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean> -->
 <!--  WIZARDS  -->
 <managed-bean>
  <managed-bean-name>wizards</managed-bean-name>
  <managed-bean-class>wizards</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
  <managed-property>
   <property-name>estatWizards</property-name>
   <list-entries/>
  </managed-property>
  <managed-property>
   <property-name>descripcioWizardActiu</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
   
 </managed-bean>
 <!--  TOOLTIPS  -->
 <managed-bean>
  <managed-bean-name>blogs</managed-bean-name>
  <managed-bean-class>blogs</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
  <managed-property>
   <property-name>estatBlogs</property-name>
   <list-entries/>
  </managed-property>
  <managed-property>
   <property-name>titolActiu</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
   <managed-property>
   <property-name>mostrar</property-name>
   <property-class>java.lang.Boolean</property-class>
   <value/>
  </managed-property>  
 </managed-bean>
 <!--  NOTICIES  -->
 <application>     
     <lifecycle>
		<phase-listener>com.degloba.gae.jsf22.SessionPhaseListener</phase-listener>
    </lifecycle>
     
     <el-resolver>
			org.springframework.web.jsf.el.SpringBeanFacesELResolver 
		</el-resolver>
		
  <locale-config>
   <default-locale>es</default-locale>
   <supported-locale>es</supported-locale>
  </locale-config>
 </application>
</faces-config>
