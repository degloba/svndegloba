<?xml version="1.0" encoding="UTF-8" ?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"    
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"    
xsi:schemaLocation="http://java.sun.com/xml/ns/persistence        
http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd" version="2.0">   
 
	<persistence-unit name="transactions-optional">        
		<provider>org.datanucleus.api.jpa.PersistenceProviderImpl</provider>        
        <class>com.degloba.domain.Modalpanel</class>
        <class>com.degloba.domain.Wizard</class>
        <class>com.degloba.canonicalmodel.publishedlanguage.AggregateId</class>
        <class>com.degloba.domain.Blog</class>
        <class>com.degloba.domain.Document</class>
        <class>com.degloba.domain.Menubar</class>
        <class>com.degloba.domain.TipusFramework</class>
        <class>com.degloba.domain.Tooltip</class>
        <class>com.degloba.domain.Framework</class>    
        <exclude-unlisted-classes>true</exclude-unlisted-classes>
		<properties>            
			<property name="datanucleus.NontransactionalRead" value="true"/>            
			<property name="datanucleus.NontransactionalWrite" value="false"/>            
			<property name="datanucleus.ConnectionURL" value="appengine"/>  			
			<property name="datanucleus.appengine.datastoreEnableXGTransactions" value="true"/>		
			      
		</properties>    
	</persistence-unit>
	
</persistence>
